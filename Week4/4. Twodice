Proceso Twodice
	
	definir dado1 Como Entero
	definir dado2 Como Entero
	definir contador Como Entero
	
	
	escribir 'Int the first number '
	leer dad1
	escribir 'Enter the secund number'
	leer dado2
	para contador =1 hasta 10 Hacer
		
		dado1 =Aleatorio(1,6)
		dado2=Aleatorio(1,6)
		
		
		Si dado1 =dado2 Entonces
			imprimir ' the dice are the same'
		SiNo
			Imprimir dado1,'',dado2
		FinSi
		
		
		
	FinPara
	
FinProceso
